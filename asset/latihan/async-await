try {
    const result = await myPromise;
    // Lakukan sesuatu dengan hasilnya
} catch (error) {
    // Tangani kesalahan
}


// Contoh implementasi async-await dalam JavaScript:
async function getData() {
    try {
        const response = await fetch('https://api.example.com/data');
        const data = await response.json();
        console.log(data);
    } catch (error) {
        console.log('Error:', error);
    }
}

getData(); //fungsi getData() dideklarasikan sebagai asynchronous dengan kata kunci async